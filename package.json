    "memdown": "^5.1.0",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "pre-commit": "1.2.x",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
