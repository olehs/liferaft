    "build": "babel ./index.js ./log.js -d ./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olehs/liferaft"
  },
  "keywords": [
    "raft",
    "liferaft",
    "lifeboat"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olehs/liferaft/issues"
  },
  "homepage": "https://github.com/olehs/liferaft",
  "dependencies": {
    "emits": "~3.0.0",
    "encoding-down": "^6.3.0",
    "eventemitter3": "^4.0.0",
    "extendible": "0.1.x",
    "immediate": "^3.2.0",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "millisecond": "0.1.x",
    "modification": "^1.0.0",
    "one-time": "^1.0.0",
    "tick-tock": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
